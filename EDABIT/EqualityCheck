public class Program
{
	public static bool CheckEquality(object a, object b)
	{
	//if(a.Equals(b)){
	//	return true;    I metodo 
	//}
	//else
	//	return false;
		
		 var result = a.Equals(b) ? true : false; //con II metodo usando ternary operator 
	 return result;
	}
}
